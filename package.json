{
  "name": "@mocha/docdash",
  "version": "1.0.0",
  "description": "A jsdoc template for mochajs, forked from docdash",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s fixtures-doc -f fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mochajs/mocha-docdash.git"
  },
  "devDependencies": {
    "jsdoc": "latest",
    "browser-sync": "latest",
    "watch-run": "latest"
  },
  "author": "Peter MÃ¼ller <munter@fumle.dk>",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ],
  "publishConfig": {
    "access": "public"
  }
}
